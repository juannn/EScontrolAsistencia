<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".AddMiembro" >

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/txtnom"
        android:layout_below="@+id/textView9"
        android:layout_marginTop="22dp"
        android:text="Nombre" />

    <EditText
        android:id="@+id/txtApe"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/txtnom"
        android:layout_below="@+id/txtnom"
        android:layout_marginTop="24dp"
        android:ems="10"
        android:textSize="11sp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/txtApe"
        android:layout_below="@+id/txtnom"
        android:text="Apellidos" />

    <EditText
        android:id="@+id/txtFecNa"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/txtApe"
        android:layout_below="@+id/txtApe"
        android:layout_marginTop="20dp"
        android:ems="10"
        android:textSize="11sp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/txtApe"
        android:layout_below="@+id/txtApe"
        android:text="Fecha Nacimiento" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/txtFecNa"
        android:layout_below="@+id/txtFecNa"
        android:text="FechaRegistro" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/txtCont"
        android:layout_alignLeft="@+id/txtCont"
        android:text="Contacto" />

    <EditText
        android:id="@+id/txtDomi"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_alignBaseline="@+id/txtApe"
        android:layout_alignBottom="@+id/txtApe"
        android:layout_alignLeft="@+id/txtCont"
        android:ems="10"
        android:textSize="11sp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView2"
        android:layout_alignBottom="@+id/textView2"
        android:layout_alignLeft="@+id/txtCont"
        android:text="Domicilio" />

    <EditText
        android:id="@+id/txtDes"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_alignBaseline="@+id/txtFecNa"
        android:layout_alignBottom="@+id/txtFecNa"
        android:layout_alignLeft="@+id/txtDomi"
        android:ems="10"
        android:textSize="11sp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/txtDomi"
        android:layout_below="@+id/txtDomi"
        android:text="Descripcion" />

    <EditText
        android:id="@+id/txtFeReg"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_alignLeft="@+id/textView4"
        android:layout_below="@+id/textView4"
        android:ems="10"
        android:textSize="11sp" >

        <requestFocus />
    </EditText>

    <EditText
        android:id="@+id/txtCont"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_above="@+id/txtApe"
        android:layout_marginLeft="38dp"
        android:layout_toRightOf="@+id/textView3"
        android:ems="10"
        android:textSize="11sp" />

    <EditText
        android:id="@+id/txtnom"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_alignParentLeft="true"
        android:layout_below="@+id/textView1"
        android:layout_marginLeft="22dp"
        android:ems="10"
        android:textSize="11sp" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignRight="@+id/textView5"
        android:text="NUEVO MIEMBRO"
        android:textSize="24sp" />

    <Button
        android:id="@+id/btnAcept"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/textView9"
        android:layout_below="@+id/txtFeReg"
        android:layout_marginTop="28dp"
        android:text="ACEPTAR" />

    <Button
        android:id="@+id/btnCance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/btnAcept"
        android:layout_toRightOf="@+id/textView3"
        android:text="CANCELAR" />

</RelativeLayout>